{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "singleAttributePerLine": true,
  "importOrder": [
    "^(react|next/(.*))$",
    "<THIRD_PARTY_MODULES>",
    "^@/constants/(.*)$",
    "^@/hooks(.*)$",
    "^@/services(.*)$",
    "^@/api(.*)$",
    "^@/config(.*)$",
    "^@/utils(.*)$",
    "^@/pages(.*)$",
    "^@/providers(.*)$",
    "^@/store(.*)$",
    "^@/assets(.*)$",
    "^@/layouts(.*)$",
    "^@/components(.*)$",
    "^@/styles(.*)$",
    "^@/shared/ui(.*)$",
    "^../(.*)$",
    "^./(.*)$",
    "(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}